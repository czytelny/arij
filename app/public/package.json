{
  "name": "arij-client",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "build": "npm run public:vendor & npm run public & npm run css",
    "public": "browserify -t [ babelify ] src/index.js -o dist/arij.js -x=react-dom -x=react -x=react-router -x=redux -x=react-redux -x=socket.io-client -x=immutable -x=underscore -d",
    "public:watch": "watchify -t [ babelify ] src/index.js -o dist/arij.js -x=react-dom -x=react -x=react-router -x=redux -x=react-redux -x=socket.io-client -x=immutable -x=underscore -d -v",
    "public:vendor": "browserify -o dist/vendor.js -r=react-dom -r=react -r=react-router -r=redux -r=react-redux -r=socket.io-client -r=immutable -r=underscore",
    "css": "lessc less/arij.less > css/arij.css & lessc less/login.less > unauthorized/css/login.css",
    "css:watch": "nodemon --watch less/*.less --ext less --exec npm run css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/czytelny/arij.git"
  },
  "author": "Michal Chwedczuk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/czytelny/arij/issues"
  },
  "homepage": "https://github.com/czytelny/arij#readme",
  "dependencies": {
    "immutable": "^3.8.1",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "redux": "^3.5.2",
    "redux-socket.io-middleware": "^1.0.2",
    "redux-thunk": "^2.1.0",
    "socket.io": "^1.4.8",
    "socket.io-client": "^1.4.8",
    "underscore": "^1.8.3",
    "whatwg-fetch": "^2.0.1",
    "when": "^3.7.7"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.16.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "del": "^2.2.2",
    "less": "^2.7.1",
    "nodemon": "^1.10.0",
    "redux-mock-store": "^1.2.1",
    "run-sequence": "^1.2.2",
    "watchify": "^3.7.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  }
}
